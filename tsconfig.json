{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "commonjs",
	  "outDir": "dist",
	  "rootDir": "src",
	  "strict": true,
	  "noImplicitAny": true,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "esModuleInterop": true,
	  "sourceMap": true,
	  "baseUrl": ".",
	  "paths": {
		"*": ["node_modules/*", "src/types/*"]
	  }
	}
  }
